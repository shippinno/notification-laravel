{
    "name": "shippinno/notification-laravel",
    "type": "library",
    "require": {
        "php": "^7.4",
        "illuminate/console": "^8.0",
        "illuminate/routing": "^8.0",
        "illuminate/support": "^8.0",
        "shippinno/notification": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Shippinno\\Notification\\Laravel\\": "src/"
        }
    },
    "extra": {
