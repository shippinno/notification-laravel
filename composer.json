{
    "name": "shippinno/notification-laravel",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "illuminate/console": "5.4.*|5.5.*|5.6.*|5.7.*",
        "illuminate/routing": "5.4.*|5.5.*|5.6.*|5.7.*",
        "illuminate/support": "5.4.*|5.5.*|5.6.*|5.7.*",
        "shippinno/notification": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Shippinno\\Notification\\Laravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shippinno\\Notification\\Laravel\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.3"
    }
}
