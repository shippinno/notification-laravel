{
    "name": "shippinno/notification-laravel",
    "type": "library",
    "repositories": [
        {
          "type": "vcs",
          "url": "git@github.com:shippinno/notification.git",
          "no-api": true
        },
        {
            "type": "vcs",
            "url": "git@github.com:shippinno/email.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "git@github.com:shippinno/template-php.git",
            "no-api": true
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/console": "^8.0",
        "illuminate/routing": "^8.0",
        "illuminate/support": "^8.0",
        "shippinno/notification": "dev-feature/symfony-mailer",
        "shippinno/email": "dev-feature/php-version-up",
        "shippinno/template": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Shippinno\\Notification\\Laravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shippinno\\Notification\\Laravel\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.3"
    }
}
