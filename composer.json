{
    "name": "shippinno/notification-laravel",
    "type": "library",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:shippinno/doctrine-json-unescaped-type.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "git@github.com:shippinno/email.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "git@github.com:shippinno/notification.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "git@github.com:shippinno/template-php.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "git@github.com:shippinno/value-objects-php.git",
            "no-api": true
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/console": "^10.0||^11.0",
        "illuminate/routing": "^10.0||^11.0",
        "illuminate/support": "^10.0||^11.0",
        "shippinno/notification": "dev-feature/php-version-up",
        "shippinno/email": "dev-feature/php-version-up",
        "shippinno/template": "dev-feature/php-version-up",
        "tanigami/doctrine-json-unescaped-type": "dev-php-version-up",
        "tanigami/value-objects": "dev-develop"
    },
    "autoload": {
        "psr-4": {
            "Shippinno\\Notification\\Laravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shippinno\\Notification\\Laravel\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.3"
    }
}
